<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:d="http://xamarin.com/schemas/2014/forms/design"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             x:Class="HomeInspectorSchedule.Pages.MetricsMenu">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Metrics"
                HorizontalOptions="Center" />

            <Button x:Name="InspectionsBtn"
                    Text="Top Inspectors"
                    Clicked="InspectionsBtn_Clicked"/>
            <Button x:Name="Realtors"
                    Text="Top Realtors"
                    Clicked="Realtors_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>